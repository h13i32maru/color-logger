{
  "source": "./src",
  "pattern": "\\.js$",
  "destination": "./out/esdoc",
  "readme": "./README.md",
  "package": "./package.json",
  "private": false,
  "styles": [],
  "scripts": [],
  "plugins": [
    {
      "name": "esdoc-importpath-plugin",
      "option": {
        "replaces": [
          {"from": "^src/", "to": "out/src/"}
        ]
      }
    }
  ]
}
